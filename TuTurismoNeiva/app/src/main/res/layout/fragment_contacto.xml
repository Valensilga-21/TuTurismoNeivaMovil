<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/backgroud"
    tools:context=".contacto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/backgroud"
        android:backgroundTint="@color/backgroud">

        <ImageView
            android:id="@+id/imageView42"
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:layout_marginStart="223dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.287"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.779"
            app:srcCompat="@drawable/circle_svgrepo_com__1_" />

        <ImageView
            android:id="@+id/imageView43"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.976"
            app:srcCompat="@drawable/ellipse_4" />

        <ImageView
            android:id="@+id/imageView44"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rotationY="180"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.023"
            app:srcCompat="@drawable/ellipse_4" />

        <ImageView
            android:id="@+id/imageView45"
            android:layout_width="15dp"
            android:layout_height="15dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.946"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.106"
            app:srcCompat="@drawable/ellipse_5" />

        <ImageView
            android:id="@+id/imageView46"
            android:layout_width="15dp"
            android:layout_height="15dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.063"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.903"
            app:srcCompat="@drawable/ellipse_5" />

        <androidx.cardview.widget.CardView
            android:layout_width="317dp"
            android:layout_height="353dp"
            app:cardBackgroundColor="@color/cards"
            app:cardCornerRadius="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.521">

            <TextView
                android:id="@+id/textView12"
                android:layout_width="231dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="40dp"
                android:text="@string/contactanos"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textColorLink="@color/white"
                android:textSize="35sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="90dp"
                android:layout_marginTop="85dp"
                android:text="@string/trabajemosJun"
                android:textColor="@color/naranja"
                android:textColorLink="@color/naranja"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="211dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="55dp"
                android:layout_marginTop="137dp"
                android:justificationMode="inter_word"
                android:text="@string/mensajeContac"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textColorLink="@color/white"
                android:textSize="16sp" />

            <LinearLayout
                android:layout_width="174dp"
                android:layout_height="60dp"
                android:layout_marginLeft="83dp"
                android:layout_marginTop="265dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/btnLlamar"
                    style="@style/Widget.AppCompat.Button.Borderless.Colored"
                    android:layout_width="42dp"
                    android:layout_height="wrap_content"
                    app:icon="@drawable/phone_svgrepo_com"
                    app:iconGravity="textStart"
                    app:iconSize="33dp"
                    app:iconTint="#FFFFFF"
                    app:iconTintMode="multiply" />

                <Button
                    android:id="@+id/btnCorreo"
                    style="@style/Widget.AppCompat.Button.Borderless.Colored"
                    android:layout_width="42dp"
                    android:layout_height="wrap_content"
                    app:icon="@drawable/gmail_svgrepo_com"
                    app:iconGravity="textStart"
                    app:iconSize="33dp"
                    app:iconTint="#FFFFFF"
                    app:iconTintMode="multiply" />

                <Button
                    android:id="@+id/btnIg"
                    style="@style/Widget.AppCompat.Button.Borderless.Colored"
                    android:layout_width="42dp"
                    android:layout_height="wrap_content"
                    app:icon="@drawable/instagram_svgrepo_com"
                    app:iconGravity="textStart"
                    app:iconSize="33dp"
                    app:iconTint="#FFFFFF"
                    app:iconTintMode="multiply" />

                <Button
                    android:id="@+id/btnWhatsApp"
                    style="@style/Widget.AppCompat.Button.Borderless.Colored"
                    android:layout_width="42dp"
                    android:layout_height="wrap_content"
                    app:icon="@drawable/whatsapp_icon_logo_svgrepo_com__1_"
                    app:iconGravity="textStart"
                    app:iconSize="33dp"
                    app:iconTint="#FFFFFF"
                    app:iconTintMode="multiply" />

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <Button
            android:id="@+id/volverContacto"
            style="@style/Widget.AppCompat.Button.Borderless.Colored"
            android:layout_width="56dp"
            android:layout_height="45dp"
            app:icon="@drawable/arrow_left_circle_svgrepo_com"
            app:iconGravity="textStart"
            app:iconSize="35dp"
            app:iconTint="@color/naranja"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.039" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>